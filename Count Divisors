//Count Divisors
#include<stdio.h>
void main()
{
    int l, r, k, i, count=0;
    scanf("%d%d%d",&l,&r,&k);
    
    if(l>=1 && r>=1 && k>=1 && l<=1000 && r<=1000 && k<=1000)
    {
        for(i=l;i<=r;i++)
        {
            if(i%k==0)
            count++;
        }
    }
    
    printf("%d",count);
}
